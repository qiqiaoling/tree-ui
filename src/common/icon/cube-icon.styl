@font-face
  font-family: "onion-icon"
  src:
    url("./onionic.woff") format("woff"),
    url("./onionic.ttf") format("truetype")

[class^="onionic-"], [class*=" onionic-"]
  font-family: "onion-icon"!important
  font-size: 100%
  font-style: normal
  -webkit-font-smoothing: antialiased
  -webkit-text-stroke-width: 0.2px
  -moz-osx-font-smoothing: grayscale

.onionic-important::before
  content: "\e68b"
.onionic-alert::before
  content: "\e676"
.onionic-ok::before
  content: "\e68c"
.onionic-back::before
  content: "\e608"
.onionic-arrow::before
  content: "\e60b"
.onionic-close::before
  content: "\e60d"
.onionic-warn::before
  content: "\e614"
.onionic-question::before
  content: "\e616"
.onionic-right::before
  content: "\e617"
.onionic-wrong::before
  content: "\e618"
.onionic-info::before
  content: "\e619"
.onionic-danger::before
  content: "\e641"
.onionic-round-border::before
  content: "\e683"
